{
  "schemaVersion": 2,
  "requiredKeys": [
    "Database:Dev:DirectConnectionString",
    "Security:ApiKey",
    "Api:BaseUrl"
  ],
  "optionalKeys": [
    "Security:Operator",
    "Integrations:Dropbox:AccessToken",
    "Integrations:Dropbox:RefreshToken",
    "Integrations:Dropbox:AppKey",
    "Integrations:Dropbox:AppSecret",
    "Integrations:Dropbox:TokenEndpoint",
    "Integrations:Dropbox:ApiRootFolder",
    "Integrations:Dropbox:UseApiRootFolder",
    "Integrations:Dropbox:LocalSyncRootHint",
    "Integrations:Dropbox:ApiBaseUrl",
    "Integrations:Dropbox:ContentBaseUrl",
    "Integrations:Email:Enabled",
    "Integrations:Email:Provider",
    "Integrations:Email:FromName",
    "Integrations:Email:ReplyTo",
    "Integrations:Email:Branding:LogoUrl",
    "Integrations:Email:Smtp:Host",
    "Integrations:Email:Smtp:Port",
    "Integrations:Email:Smtp:UseSsl",
    "Integrations:Email:Smtp:User",
    "Integrations:Email:Smtp:Password",
    "Integrations:Email:Gmail:ServiceAccountEmail",
    "Integrations:Email:Gmail:PrivateKey",
    "Integrations:Email:Gmail:ImpersonatedUser",
    "Integrations:Email:Gmail:TokenUri"
  ],
  "globalPolicy": {
    "allowedDbConnectionKeyPatterns": [
      "*Dev*",
      "*Developer*",
      "*Local*"
    ],
    "disallowedKeyPatterns": [
      "*Prod*",
      "*Production*",
      "*Staging*",
      "*CI*",
      "*Github*",
      "*GitHub*"
    ]
  }
}
